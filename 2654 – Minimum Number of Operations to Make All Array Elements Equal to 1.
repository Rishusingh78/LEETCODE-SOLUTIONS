class Solution {
public:
    int gcd(int a, int b) {
        return b == 0 ? a : gcd(b, a % b);
    }

    int minOperations(vector<int>& nums) {
        int n = nums.size();

        int totalGcd = nums[0];
        for (int i = 1; i < n; i++) {
            totalGcd = gcd(totalGcd, nums[i]);
        }
        if (totalGcd != 1) return -1;

        int ones = 0;
        for (int x : nums) {
            if (x == 1) ones++;
        }
        if (ones > 0) return n - ones;

        int minLen = n;
        for (int i = 0; i < n; i++) {
            int g = nums[i];
            for (int j = i + 1; j < n; j++) {
                g = gcd(g, nums[j]);
                if (g == 1) {
                    minLen = min(minLen, j - i + 1);
                    break;
                }
            }
        }

        return n + minLen - 2;
    }
};
